{
  "name": "@srclaunch/transform",
  "description": "Data and value transformation utilities",
  "author": "Steven Bennett <steven@srclaunch.com>",
  "license": "MIT",
  "version": "0.1.2",
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "package.json",
    "dist"
  ],
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.umd.js"
    }
  },
  "scripts": {
    "build": "yarn srclaunch build",
    "qr": "yarn build && yarn srclaunch changesets add --message \"Quick release - patch\" && yarn srclaunch release",
    "release": "yarn test:coverage && yarn srclaunch release",
    "test": "yarn srclaunch test",
    "test:watch": "yarn srclaunch test --watch",
    "test:coverage": "yarn srclaunch test --coverage",
    "yui": "yarn upgrade-interactive"
  },
  "devDependencies": {
    "@srclaunch/dx": "^0.1.2",
    "@srclaunch/types": "^0.1.2",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-stylelint": "^15.0.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^4.2.0",
    "eslint-plugin-github": "^4.3.5",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-regexp": "^1.5.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-sort-requires": "^2.1.0",
    "eslint-plugin-testing-library": "^5.0.5",
    "eslint-plugin-unicorn": "^40.1.0",
    "prettier": "^2.5.1",
    "prettier-eslint": "^13.0.0",
    "standard-version": "^9.3.2",
    "stylelint": "^14.3.0",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^6.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.5.5",
    "vite": "^2.7.13"
  }
}
